#include <stdio.h>
int main()
double a,b;
char op;
printf("Enter a : ");
scanf("%lf",&a);
printf("Enter b : ");
scanf("%lf",&b);
printf("Enter operator : ");
scanf("%c",&op);
switch(op)
{
case '+':
printf("%lf",a+b);
break;
case '-':
printf("%lf",a-b);
break;
case '*':
printf("%lf",a*b);
break;
case '/':
printf("%lf",a/b);
break;
case '%':
printf("%lf",a%b);
break;
default:
printf("Invalid");
}
return 0;
}
  
